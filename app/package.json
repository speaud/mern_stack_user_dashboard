{
  "name": "mern_stack_user_dashboard",
  "version": "1.0.0",
  "description": "mern_stack_user_dashboard",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./client/etc/webpack.config.js --env.NODE_ENV=production --progress",
    "client": "webpack-dev-server --config ./client/etc/webpack.config.js --env.NODE_ENV=development",
    "server": "nodemon ./server/server.js",
    "start-servers": "node common/etc/start-servers",
    "todo-finder": "node common/etc/todo-finder",
    "update-jsdoc": "jsdoc -c ./common/etc/jsdoc.config.json",
    "open-jsdoc": "xdg-open ./common/doc/jsdoc/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speaud/mern_stack_user_dashboard.git"
  },
  "author": "Joshua Mummert",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/speaud/mern_stack_user_dashboard/issues"
  },
  "homepage": "https://github.com/speaud/mern_stack_user_dashboard#readme",
  "dependencies": {
    "axios": "^0.17.0",
    "bcrypt-nodejs": "0.0.3",
    "es6-promise": "^4.1.1",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.1.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "~1.18.2",
    "chalk": "^2.3.0",
    "connect-mongo": "^2.0.0",
    "css-loader": "^0.28.4",
    "express": "~4.16.1",
    "express-session": "^1.15.6",
    "html-minifier": "^3.5.6",
    "html-webpack-plugin": "^2.30.1",
    "jsdoc": "^3.5.5",
    "material-ui": "^0.19.4",
    "mongoose": "~4.11.14",
    "monk": "^6.0.5",
    "morgan": "~1.9.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-es2015-spread"
    ]
  },
